{
  "game_name": "Tetris",
  "author": "bob",
  "version": "1.0.0",
  "description": "Two-player Tetris duel with GUI client.",
  "type": "GUI",
  "max_players": 2,
  "lobby_requires_download": true,
  "uploaded": true,
  "server": {
    "command": "python3 server.py --port {port} --room {room_id} --token {token} --p1 {p1} --p2 {p2}",
    "working_dir": ".",
    "env": {
      "ROOM_ID": "{room_id}",
      "PORT": "{port}",
      "TOKEN": "{token}",
      "PLAYER1": "{p1}",
      "PLAYER2": "{p2}"
    }
  },
  "client": {
    "command": "python3 client.py --host {host} --port {port} --player {player_name} --token {token} --gui",
    "working_dir": ".",
    "env": {
      "PLAYER_NAME": "{player_name}",
      "ROOM_TOKEN": "{token}"
    }
  },
  "assets": [],
  "healthcheck": {
    "tcp_port": "{port}",
    "timeout_sec": 5
  }
}